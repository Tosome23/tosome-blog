<h1>투삼로지스 블로그</h1>
<ul>
{% for post in collections.posts | reverse %}
  <li>
    <a href="{{ post.url }}">{{ post.data.title }}</a>
    <small> · {{ post.date | date("YYYY-MM-DD") }}</small>
    {% if post.data.description %}<br>{{ post.data.description }}{% endif %}
  </li>
{% endfor %}
</ul>
